package {	import flash.display.MovieClip;	import flash.events.*;	import flash.system.System;    import flash.desktop.ClipboardFormats;		public class parse extends MovieClip {		public function clearBtnHandler(e:MouseEvent){			inputField.htmlText ="";		}				function copyBtnHandler(e:MouseEvent){		    inputField.setSelection(0, inputField.text.length)		    System.setClipboard(inputField.text);		}			}}