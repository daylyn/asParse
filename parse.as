package {	import flash.display.MovieClip;	import flash.events.*;	import flash.system.System;    import flash.desktop.ClipboardFormats;	//new changes	public class parse extends MovieClip {		public function clearBtnHandler(e:MouseEvent){			inputField.htmlText ="";			inputField.htmlText ="new changes";		}				function copyBtnHandler(e:MouseEvent){		    inputField.setSelection(0, inputField.text.length)		    System.setClipboard(inputField.text);		}			}}